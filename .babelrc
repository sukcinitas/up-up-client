{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": { "version": 3 },
        "targets": "defaults"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "corejs": 3 }],
    "@babel/plugin-transform-typescript",
    "@babel/plugin-proposal-class-properties"
  ]
}
